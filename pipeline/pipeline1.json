{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "ds_vendas",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "ds_net",
						"type": "DatasetReference",
						"parameters": {
							"containerName": {
								"value": "@pipeline().DataFactory",
								"type": "Expression"
							},
							"filesName": {
								"value": "@pipeline().Pipeline",
								"type": "Expression"
							},
							"separatorName": ";"
						}
					}
				]
			}
		],
		"folder": {
			"name": "Estudos_pipe"
		},
		"annotations": [],
		"lastPublishTime": "2022-01-04T17:34:28Z"
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
